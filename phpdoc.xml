<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor configVersion="3">

  <title>Documentation Symfony MM</title>

  <paths>
    <output>phpdoc-output</output>
    <cache>phpdoc-output/cache</cache>
  </paths>

  <templates>
    <template>
      <name>clean</name>
    </template>
  </templates>

  <versions>
    <version number="1.0">
      <api>
        <source dsn=".">
          <path>src/Controller</path>
          <path>src/Service</path>
        </source>
      </api>
    </version>
  </versions>

</phpdocumentor>
